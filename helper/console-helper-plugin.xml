<plugin
	name="console-helper"
	package="wbs.console.helper">

	<layer name="object-generate">

		<component class="generate.ConsoleHelperGenerator"/>

	</layer>

	<layer name="console">

		<bootstrap-component class="provider.ConsoleHelperProviderComponentBuilder"/>
		<bootstrap-component class="provider.ConsoleHelperProviderSpec"/>
		<bootstrap-component class="provider.PrivKeyComponentBuilder"/>
		<bootstrap-component class="provider.PrivKeySpec"/>
		<bootstrap-component class="provider.RunPostProcessorSpec"/>

		<component class="builder.PrivKeyBuilder"/>

		<component class="core.ConsoleHelperImplementation"/>

		<component class="manager.ConsoleObjectManagerImplementation"/>

		<component class="provider.GenericConsoleHelperProvider"/>

	</layer>

</plugin>
